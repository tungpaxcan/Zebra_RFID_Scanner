    <script>
        window.addEventListener('offline', function () {
            Swal.fire("@Html.DisplayName(CollTex.Resources.Resource.internetdisconnect)", "@Html.DisplayName(CollTex.Resources.Resource.pleasereconnecttothenetwork)", "error");
        })
        window.addEventListener('online', function () {
            Swal.fire("@Html.DisplayName(CollTex.Resources.Resource.internetconnection)", "&#9989;", "success");
        })
    </script>


    <script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.4.1/jspdf.min.js"></script>
    <script src="~/Scripts/Validate.js"></script>

    <script>
        var confirmVar = "@Html.DisplayName(CollTex.Resources.Resource.confirm)";
        var closeVar = "@Html.DisplayName(CollTex.Resources.Resource.close)";
        var pressButtonVar = "@Html.DisplayName(CollTex.Resources.Resource.pressthebutton)";
        var cancelVar = "@Html.DisplayName(CollTex.Resources.Resource.cancel)";
        $(document).on({
            ajaxStart: function () {
                $("#wait").show();
            },
            ajaxStop: function () {
                $("#wait").hide();
            }
        });
        $(function LoadUser() {
            $.ajax({
                url: '/Home/UserSession',
                type: 'get',
                success: function (data) {
                    if (data.code == 200) {
                        $('span[name="nameUser"]').append(data.name)
                        $('span[name="firstuser"]').append(data.name.trim().lastIndexOf(" ") == -1 ? data.name.substring(-1, 1) : data.name.substring(data.name.trim().lastIndexOf(" ") + 1, data.name.trim().lastIndexOf(" ") + 2))
                        if (data.admin == false) {
                            $('div[name="admin"]').css("display", "none");
                            $('a[name="admin"]').css("display", "none");
                            $('div[name="authorizationUser"]').append("@Html.DisplayName(CollTex.Resources.Resource.staff)");
                        } else {
                            $('div[name="authorizationUser"]').append("@Html.DisplayName(CollTex.Resources.Resource.manage)");
                        }
                    }
                    else {
                        Swal.fire(data.msg, "@Html.DisplayName(CollTex.Resources.Resource.pressthebutton)", "error");
                    }
                },
            })
        })

        function ChangePass() {
            let oldPass = $('#oldPass').val().trim()
            let newPass = $('#newPass').val().trim()
            let againNewPass = $('#againNewPass').val().trim()
            if (newPass.length < 6) {
                Swal.fire("@Html.DisplayName(CollTex.Resources.Resource.newpasswordmustbegreaterthan6characters) !!!", "@Html.DisplayName(CollTex.Resources.Resource.pressthebutton)", "warning"); $('#newPass').css('border-color', "red"); return;
            } else { $('#newPass').css('border-color', "green") }
            if (newPass != againNewPass) {
                Swal.fire("@Html.DisplayName(CollTex.Resources.Resource.reenterincorrectpassword) !!!", "@Html.DisplayName(CollTex.Resources.Resource.pressthebutton)", "warning"); $('#againNewPass').css('border-color', "red"); return;
            } else { $('#againNewPass').css('border-color', "green") }
            $.ajax({
                url: '/Register/ChangePass',
                type: 'post',
                data: { oldPass, newPass },
                success: function (data) {
                    if (data.code == 200) {
                        Swal.fire({
                            title: data.msg,
                            icon: "success",
                            buttonsStyling: false,
                            confirmButtonText: `<a style="color:white" href="/Home/Index">@Html.DisplayName(CollTex.Resources.Resource.confirm)</a>`,
                            customClass: {
                                confirmButton: "btn btn-primary"
                            }
                        });

                    } else if (data.code == 300) {
                        Swal.fire(data.msg, "@Html.DisplayName(CollTex.Resources.Resource.pressthebutton)", "error");
                        $('#oldPass').css('border-color', "red")
                    }
                    else {
                         Swal.fire(data.msg, "@Html.DisplayName(CollTex.Resources.Resource.pressthebutton)", "error");
                    }
                },
            })
        }
    </script>
    <script src="~/Scripts/Layout.js"></script>
    @RenderSection("scripts", required: false)
